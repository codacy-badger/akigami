.menu-item {
    text-transform: uppercase;
    letter-spacing: -0.4px;
    font-weight: 500;
}

.container-nav {
    width: 100%;
    @media screen and (min-width: 768px) {
        width: 720px;
    }
    @media screen and (min-width: 992px) {
        width: 960px;
    }
    @media screen and (min-width: 1200px) {
        width: 1140px;
    }
}